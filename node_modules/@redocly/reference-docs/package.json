{
  "name": "@redocly/reference-docs",
  "version": "2.19.1",
  "description": "Redocly Reference Docs",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "author": "Roman Hotsiy <roman@redoc.ly>",
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">=12",
    "npm": ">=6.0.0"
  },
  "bin": {
    "redocly-api-reference": "bin/cli/commands/redocly-docs-cli.min.js",
    "reference-docs": "bin/cli/commands/redocly-docs-cli.min.js"
  },
  "dependencies": {
    "@redocly/openapi-core": "1.0.0-beta.97",
    "@redocly/vscode-json-languageservice": "3.4.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.65.0",
    "decko": "^1.2.0",
    "deepmerge": "^4.2.2",
    "dompurify": "^2.0.12",
    "eventemitter3": "^4.0.4",
    "fast-deep-equal": "^3.1.3",
    "handlebars": "^4.7.6",
    "hotkeys-js": "^3.7.6",
    "informed": "3.33.0",
    "json-pointer": "^0.6.2",
    "kleur": "^3.0.3",
    "lunr": "^2.3.8",
    "mark.js": "^8.11.1",
    "marked": "^4.0.10",
    "mkdirp": "^0.5.1",
    "mobx-react": "^7.2.0",
    "node-libs-browser": "^2.2.0",
    "openapi-sampler": "^1.2.3",
    "path-browserify": "^1.0.1",
    "perfect-scrollbar": "^1.5.5",
    "polished": "^3.6.5",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.6",
    "react-codemirror2": "^7.2.1",
    "react-tabs": "^3.1.1",
    "serve-static": "^1.14.1",
    "slugify": "^1.4.4",
    "stickyfill": "^1.1.1",
    "stringify-object": "^3.3.0",
    "styled-components": "^4.1.1 || ^5.1.1",
    "swagger-client": "^3.13.3",
    "swagger2openapi": "^7.0.8",
    "tslib": "^2.2.0",
    "url-template": "^2.0.8",
    "yargs": "^17.3.0"
  },
  "scripts": {
    "prepublishOnly-local": "npm run clean && npm run bundle && npm run compile-lib && npm run bundle-cli && npm run prepublish-test",
    "clean": "rm -rf bin dist lib lib-esm cli-zip redocly-static",
    "bundle": "webpack --mode=production",
    "compile-lib": "npm run compile-es5 && npm run compile-esm && npm run bundle-worker",
    "compile-es5": "rm -rf lib && tsc -p tsconfig.lib.json && node scripts/minify.js lib",
    "compile-esm": "rm -rf lib-esm && tsc -p tsconfig.lib-esm.json && node scripts/minify.js lib-esm",
    "bundle-worker": "webpack --config webpack.config.worker.js --mode=production",
    "bundle-cli": "webpack --config webpack.config.server.js --mode=production && cp -Rf src/cli/assets bin/cli/assets",
    "publish-cli-s3": "scripts/publish-cli-s3.sh",
    "deploy-local": "ENV=local npm run bundle-cli && ENV=local npm run publish-cli-s3",
    "publish-cdn": "scripts/publish-cdn.sh",
    "pack-sourcemaps": "rm -rf reference-docs.sourcemaps.tag.gz && find lib lib-esm -name \"*.js.map\" | xargs tar -czvf reference-docs.sourcemaps.tar.gz",
    "start": "webpack serve --mode=development --hot",
    "stats": "webpack --json --profile --mode=production -p > stats.json && npx webpack-bundle-analyzer stats.json",
    "prettier": "prettier --write \"src/cli/cli.ts\" \"src/**/*.{ts,tsx}\" \"playground/**/*.{ts,tsx}\"",
    "lint": "eslint --fix . --ext .ts,.tsx --cache",
    "prepublish-test": "scripts/prepublish-test.sh",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:e2e": "percy exec -- cypress run",
    "cypress": "cypress open",
    "list-licenses": "node ./scripts/list-licenses.js",
    "license-check": "license-checker --production --onlyAllow 'MIT;ISC;Apache-2.0;Apache;CC0-1.0;BSD;BSD-2-Clause;BSD-3-Clause;UNKNOWN' --summary",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "precdn-serve": "mkdir -p playground/cdn-test/server-dist && cp -Rf dist/* playground/cdn-test/server-dist && cp -Rf playground/cdn-test/template-source/* playground/cdn-test/server-dist",
    "cdn-serve": "http-server playground/cdn-test/server-dist",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && npm run lint"
  },
  "peerDependencies": {
    "core-js": "^3.1.4",
    "mobx": "^6.3.2",
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0",
    "styled-components": "^4.1.1 || ^5.1.1"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.68",
    "@percy/cypress": "^3.1.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/codemirror": "0.0.88",
    "@types/dompurify": "^2.0.2",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^25.1.4",
    "@types/json-pointer": "^1.0.30",
    "@types/mark.js": "^8.11.5",
    "@types/marked": "^4.0.1",
    "@types/node": "^14.14.37",
    "@types/prismjs": "^1.16.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.4",
    "@types/react-tabs": "^2.3.2",
    "@types/styled-components": "^5.1.9",
    "@types/webpack": "^4.41.9",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "core-js": "^3.5.0",
    "css-loader": "^6.5.1",
    "cypress": "^8.4.0",
    "cypress-file-upload": "^5.0.8",
    "enzyme": "^3.7.0",
    "enzyme-to-json": "^3.6.2",
    "esbuild-loader": "^2.16.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "json-schema": "^0.4.0",
    "mobx": "^6.3.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^17.0.2",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^1.0.1",
    "terser": "^5.6.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.8.1",
    "typescript": "4.4.2",
    "unfetch": "^4.1.0",
    "url-polyfill": "^1.1.7",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "workerize-loader": "github:redocly/workerize-loader#webpack-5-dist",
    "yaml-js": "^0.2.3"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    },
    "testPathIgnorePatterns": [
      "cypress"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "\\.d\\.ts$",
      "/benchmark/",
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/benchmark/",
      "src/components/__tests__/mocks/*",
      "src/models/*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/empty.js"
    }
  }
}
