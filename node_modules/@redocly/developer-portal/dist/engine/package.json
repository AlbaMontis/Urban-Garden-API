{
  "name": "@redocly/developer-portal",
  "version": "1.1.0-beta.96",
  "description": "Redocly Developer Portal",
  "author": "redoc.ly <team@redoc.ly>",
  "license": "SEE LICENSE IN LICENSE",
  "bin": {
    "redocly-portal": "dist/cli.js"
  },
  "scripts": {
    "start": "node cli.js develop -p 3000",
    "start-test-server": "REDOCLY_TEST_CONTENT_DIR=tests/starter npm start",
    "start-test-server-simple": "REDOCLY_TEST_CONTENT_DIR=tests/simple npm start",
    "start-test-server-apigee": "REDOCLY_TEST_CONTENT_DIR=tests/apigee npm start",
    "start-test-server-catalog": "REDOCLY_TEST_CONTENT_DIR=tests/catalog npm start",
    "start-test-server-multiversions": "REDOCLY_TEST_CONTENT_DIR=tests/multiversions npm start",
    "multiversions:build": "REDOCLY_TEST_CONTENT_DIR=../tests/multiversions node dist/cli.js build",
    "starter:build": "REDOCLY_TEST_CONTENT_DIR=../tests/starter node dist/cli.js build",
    "starter-prefix-path:build": "REDOCLY_TEST_CONTENT_DIR=tests/starter REDOCLY_PREFIX_PATHS=/public npm run build",
    "starter:serve": "node dist/cli.js serve -p 3000",
    "apigee:build": "REDOCLY_TEST_CONTENT_DIR=../tests/apigee node dist/cli.js build",
    "build": "node cli.js build",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,yml,yaml}\"",
    "lint": "tslint --project tsconfig.json \"./engine/src/**/*.ts\" \"./engine/src/**/*.tsx\"",
    "update-types": "graphql-codegen --config codegen.yml",
    "compile": "tsc -p tsconfig.dist.json",
    "copy-deps": "cp engine/package.json dist/engine && find engine/plugins engine/images engine/src \\( -name \\*.json -o -name \\*.svg -o -name \\*.html \\) -exec rsync -R {} dist \\;",
    "minify": "babel-minify dist -d dist --sourceType module --builtIns=false",
    "prepublishOnly": "rm -rf dist && npm run compile && npm run minify && npm run copy-deps",
    "minify-server": "babel-minify portal-server/*.js -d portal-server/dist",
    "build-docker-server": "npm run minify-server && docker build -t redocly/portal-server portal-server",
    "build-docker": "npm run prepublishOnly && docker build -t redocly-portal -f docker/Dockerfile . ",
    "clean": "node cli.js clean",
    "test": "npm run test:unit && percy exec -- ./node_modules/.bin/cypress run",
    "cypress": "./node_modules/.bin/cypress open",
    "test:unit:watch": "jest --watch",
    "test:unit": "jest --coverage",
    "test:update-snapshot": "jest --updateSnapshot",
    "license-check": "license-checker --production --onlyAllow 'MIT;ISC;Apache-2.0;Apache;CC-BY-4.0;Unlicense;CC-BY-3.0;CC0-1.0;Public Domain;WTFPL;BSD;BSD-2-Clause;BSD-3-Clause;Custom: https://redoc.ly/subscription-agreement/;UNKNOWN' --summary",
    "list-licenses": "node ./scripts/list-licenses.js",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,yml,yaml}\""
  },
